

INPUT


variable_group x, y, z;
constant pi_1_1, pi_1_2, pi_1_3, pi_1_4;
pi_1_1 = 0;
pi_1_2 = 6.-3;
pi_1_3 = 9.-1;
pi_1_4 = -3.-1;
constant pi_2_1, pi_2_2, pi_2_3, pi_2_4;
pi_2_1 = 0;
pi_2_2 = -7.-1;
pi_2_3 = 2.-1;
pi_2_4 = 6.-1;
function f1;
f1 = x^2 + y^2 + z^2 - 1;
function der_func;
der_func = 2*pi_1_3*pi_2_4*x - 2*pi_1_4*pi_2_3*x - 2*pi_1_2*pi_2_4*y + 2*pi_1_4*pi_2_2*y + 2*pi_1_2*pi_2_3*z - 2*pi_1_3*pi_2_2*z;
END;


constant synthetic_var_patch_1, synthetic_var_patch_2, synthetic_var_patch_3;
synthetic_var_patch_1 = -2.-1+I*-4.-1;
synthetic_var_patch_2 = 8.-1+I*2.-1;
synthetic_var_patch_3 = 5.-2+I*-5.-1;
function synth_var_patch;
synth_var_patch = synthetic_var_patch_1 * synth1 + synthetic_var_patch_2 * synth2 + synthetic_var_patch_3 * synth3;


